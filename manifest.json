{
  "manifest_version": 2,
  "name": "Remove Specific Elements",
  "version": "1.0",
  "description": "Removes elements with a certain class from web pages",
  "permissions": ["activeTab", "<all_urls>"],
  "web_accessible_resources": [
      "banned.json"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
